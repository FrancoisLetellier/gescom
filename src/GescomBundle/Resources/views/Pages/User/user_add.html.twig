{% extends '@Gescom/Default/base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
    <div id="user_register">
        {{ form_start(form) }}

        {{ form_row(form.username) }}
        {{ form_row(form.plainPassword.first) }}
        {{ form_row(form.plainPassword.second) }}
        {{ form_row(form.submit) }}

        {{ form_end(form) }}
    </div>
{% endblock %}